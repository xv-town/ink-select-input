{
  "name": "@xv-town/ink-input-select",
  "version": "0.0.0",
  "description": "Select input component for Ink",
  "keywords": [
    "ink",
    "ink-component"
  ],
  "author": "xiaoyown <qzy09101018@gmail.com>",
  "homepage": "https://github.com/xv-town/ink-select-input#readme",
  "license": "MIT",
  "main": "./build/index.js",
  "files": [
    "./build"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xv-town/ink-select-input.git"
  },
  "scripts": {
    "dev": "tsc --watch --declaration",
    "build": "tsc --declaration",
    "compile": "tsc",
    "lint": "npm run lint:ts && npm run lint:es && npm run lint:prettier",
    "lint:ts": "npm run compile -- --noEmit",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx ./ --cache --quiet",
    "lint:prettier": "prettier --list-different \"**/*.{js,jsx,ts,tsx,css,less,json,yml,yaml,md}\"",
    "fix": "npm run fix:es && npm run fix:prettier",
    "fix:es": "eslint --ext .js,.jsx,.ts,.tsx ./ --cache --quiet --fix",
    "fix:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css,less,json,yml,yaml,md}\""
  },
  "bugs": {
    "url": "https://github.com/xv-town/ink-select-input/issues"
  },
  "devDependencies": {
    "@types/ink": "^2.0.3",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ink": "^3.0.9",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "ink": "^3.0.9",
    "react": "^17.0.2"
  },
  "dependencies": {
    "arr-rotate": "^1.0.0",
    "figures": "3.2.0",
    "lodash.isequal": "^4.5.0"
  }
}
